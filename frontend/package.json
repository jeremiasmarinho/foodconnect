{
  "name": "frontend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "build:production": "NODE_ENV=production next build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false --passWithNoTests --reporters=default --reporters=jest-junit",
    "lint": "echo 'Linting not configured yet'",
    "performance:bundle": "npm run build:analyze",
    "performance:lighthouse": "lighthouse http://localhost:3000 --output json --output-path ./performance/lighthouse-report.json",
    "ci:test": "npm run test:ci",
    "ci:quality": "npm run lint",
    "ci:all": "npm run ci:quality && npm run ci:test",
    "ci": "npm run typecheck && npm run test:ci",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.7",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/native-stack": "^7.3.26",
    "@tanstack/react-query": "^5.90.2",
    "axios": "^1.12.2",
    "expo": "~54.0.12",
    "expo-image-picker": "^17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-navigation-bar": "^5.0.8",
    "expo-splash-screen": "^31.0.10",
    "expo-status-bar": "~3.0.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.0",
    "babel-jest": "^30.2.0",
    "babel-preset-expo": "^54.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "~29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-junit": "^16.0.0",
    "jsdom": "^27.0.0",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.2"
  },
  "jest-junit": {
    "outputDirectory": "./test-results",
    "outputName": "junit.xml"
  },
  "overrides": {
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-test-renderer": "19.1.0"
  },
  "private": true
}
